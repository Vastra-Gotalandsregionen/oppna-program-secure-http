<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <bean class="se.vgregion.secure.util.KeystoreHelper">
    <constructor-arg index="0" value="JKS"/>
    <constructor-arg index="1" value="classpath:teststore.jks"/>
    <constructor-arg index="2" value="test_alias"/>
    <constructor-arg index="3" value="changeit"/>
  </bean>

</beans>
